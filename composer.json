{
    "name": "markhuot/twig-modules",
    "description": "An easy way to load CSS Module JSON files in to Twig",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin"
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "webonyx/graphql-php": "^0.9.10",
        "react/http": "^0.7",
        "anahkiasen/underscore-php": "^2.0",
        "symfony/serializer": "^3.3",
        "symfony/property-access": "^3.3"
    },
    "authors": [
        {
            "name": "Mark Huot",
            "email": "mark@markhuot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "markhuot\\CraftQL\\": "src/"
        }
    },
    "extra": {
        "name": "Twig Modules",
        "handle": "twig-modules",
        "developer": "Mark Huot",
        "developerUrl": "https://www.github.com/markhuot",
        "class": "markhuot\\TwigModules\\Plugin"
    }
}
